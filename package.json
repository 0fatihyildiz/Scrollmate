{
  "name": "scrollmate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@types/node": "^18.14.2",
    "eslint": "^8.41.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.1",
    "prettier": "latest",
    "turbo": "latest",
    "unocss": "^0.52.7"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.32.3",
  "dependencies": {
    "@unocss/reset": "^0.52.7",
    "scrollmate": "workspace:^"
  }
}
